<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="matem.chartkick">

  <browser:page
    name="example.highcharts"
    for="*"
    template="highcharts.pt"
    permission="zope2.View"
    />

  <browser:page
    name="example.chartkick"
    for="*"
    template="chartkick.pt"
    permission="zope2.View"
    />

</configure>